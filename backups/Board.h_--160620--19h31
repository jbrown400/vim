/*
 * Houses the Board class
 */

class Board {
    private:

    public:
        char GameBoard[8][8];
        Board() {
            //For every game mode, the board initialization will be the same.
            for (int i = 0; i < 8; i++) {
                for (int j = 0; j < 8; j++) {
                    GameBoard = 'X';
                }
            }
        }
};
